{
    "name": "apielf/hyperf-query-builder",
    "description": "Build Hyperf database queries from API requests",
    "keywords": [
        "hyperf",
        "api",
        "query",
        "builder",
        "filter",
        "sort",
        "include"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "daixinguo",
            "email": "daixinguo57@163.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/database": "^3.0",
        "hyperf/http-server": "^3.0",
        "hyperf/di": "^3.0",
        "hyperf/config": "^3.0",
        "hyperf/collection": "^3.0",
        "hyperf/stringable": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ApiElf\\QueryBuilder\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
